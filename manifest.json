{
    "name": "bouncing balls",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["out/main.js"]
      }
    ],
  
    "manifest_version": 2
  }